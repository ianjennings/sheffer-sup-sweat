{
  "name": "Supreme Hyper Sweat",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Out-sweat all the normcores by becoming the fastest hype beast.",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "default_locale": "en",  "permissions": [
    "cookies",
    "tabs",
    "http://www.supremenewyork.com/shop/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.supremenewyork.com/*",
        "https://www.supremenewyork.com/*"
      ],
      "js": [
        "js/jquery/jquery.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
